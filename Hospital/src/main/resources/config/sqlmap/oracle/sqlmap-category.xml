<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CategoryVO">

	
	<!-- hospitalCategory -->
	<select id="selectCategory" parameterType="CategoryVO" resultType="CategoryVO">
		select *
		 from h_category
		   order by sort_no asc
	</select>
	
	<!-- SymptomCategory -->
	<select id="selectSymCategory" parameterType="CategoryVO" resultType="CategoryVO">
		select *
		 from s_category
		   order by sort_no asc
	</select>
	


</mapper>